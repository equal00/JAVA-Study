package loop;

public class Countinue1 {
    public static void main(String[] args) {
        int i = 1;

        while (i <= 5) {
            if (i == 3) {
                i++;
                continue;
            }// i == 3이면 아래 코드 무시, 조건식으로 이동
            System.out.println(i);
            i++;
        }
    }
}
