package cond;

public class Switch3 {
    public static void main(String[] args) {
        int grade = 2;

        int coupon;

        switch (grade) {
            case 1:
                coupon = 1000;
                break;
            case 2:
            case 3:
                coupon = 3000;
            default:
                coupon = 500;
        }
        System.out.println("발급받은 쿠폰: " + coupon );
    }
}

/*
break문이 없을 경우
 : 다음 코드가 실행됨
case 2 = 3000
case 3 = 3000

*/
